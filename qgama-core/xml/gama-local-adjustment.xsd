  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref='from'/>
    <xsd:element ref='to'/>
    <xsd:element ref='obs'/>
    <xsd:element ref='adj'/>
    <xsd:element ref='stdev'/>
    <xsd:element ref='qrr'/>
    <xsd:element ref='f'/>
    <xsd:element ref='std-residual'/>
